{
  "private": true,
  "scripts": {
    "start": "npm run clean && npm run modernizr && webpack",
    "build": "npm run clean && npm run modernizr && NODE_ENV=production webpack",
    "clean": "rm -rf dist",
    "modernizr": "modernizr -c modernizr.json -d dist/modernizr.js",
    "postinstall": "find node_modules/ -name '*.info' -type f -delete",
    "test": "eslint js && sasslint scss/**/*.scss"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "@dosomething/eslint-config"
  },
  "dependencies": {
    "chart.js": "~1.0.2",
    "@dosomething/forge": "6.7.0-0",
    "dosomething-modal": "~0.3.0",
    "dosomething-validation": "~0.2.0",
    "dotty": "0.0.2",
    "es5-shim": "^4.1.1",
    "fixed-sticky": "DFurnes/fixed-sticky#umd",
    "html5shiv": "aFarkas/html5shiv.git#3.7.2",
    "jquery": "~1.11.0",
    "jquery-once": "~2.0.0",
    "jquery.iecors": "DFurnes/jquery.iecors#umd",
    "lodash": "~3.6.0",
    "mailcheck": "~1.1.1",
    "raf.js": "~0.0.4",
    "respond.js": "~1.4.2",
    "unveil": "DFurnes/unveil.git#umd"
  },
  "devDependencies": {
    "@dosomething/eslint-config": "^1.1.1",
    "@dosomething/webpack-config": "^1.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-plugin-react": "^4.2.3",
    "modernizr": "^3.3.1",
    "raw-loader": "^0.5.1",
    "sasslint": "0.0.10",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
